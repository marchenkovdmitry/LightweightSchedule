  .btn-container
    .setup-general.li-setup
    .setup-general.background-setup
    .setup-general.day-setup
  .setup-general.menu-setup
  
  .wrapper-main
  
    .main_title#monday Понедельник
    ul.day_week
      li
        .num 1.Ч
        .time #{time.first}
        .discipline #{empty.title}
        
